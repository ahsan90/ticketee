<div class="container">
<h2>New Project</h2>
<%= form_for(@project) do |f| %>
<div class="row">
<div>
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>
<div>
  <%= f.label :description %><br />
  <%= f.text_field :description %>
</div>
      <%= f.submit %>
<% end %>
</div>
</div>